{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "ecmaFeatures": { "modules": true },
  "rules": {
    "no-undef": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-vars": 1,
    "comma-dangle": [1, "always-multiline"],
    "no-mixed-spaces-and-tabs": 1,
    "key-spacing": 1,
    "no-shadow": 1,
    "no-bitwise": 1,
  }
}
